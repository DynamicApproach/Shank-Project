define fib (n : integer; var ans: integer)
variables
    z : integer
begin
    x:= 0
    y:= 1
if n < 0
    begin
        ans:= 0
    end
elsif n = 0 then
    begin
    ans := 0
    end
elsif n = 1 then
    begin
    ans := y
    end
else
    begin
    if counter < n
    z = x + y
    x = y
    y = z
    ans := y
    end
end